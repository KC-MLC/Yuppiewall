<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.total {
			font-size: 1em;
			font-weight: bold;
		}
	</ui:style>

	<g:VerticalPanel width="100%" ui:field="accountList">
		<g:HTMLPanel>
			<table width="100%">
				<tr>
					<td align="left" class='{style.total}'>Total</td>
					<td align="right" class='{style.total}'>
						<g:Label ui:field="laAccountTotal" />
					</td>
				</tr>
				<tr>
					<td align="left" class='{style.total}'>Today</td>
					<td align="right" class='{style.total}'>
						<g:Label ui:field="laAccountTotalChange" />
					</td>
				</tr>
			</table>
		</g:HTMLPanel>

	</g:VerticalPanel>
</ui:UiBinder> 